<%= form_for @jamsession do |f| %>
<%= f.label :title, "Your Session Name" %>
<%= f.text_field :title %><br>
<%= f.label :description %>
<%= f.text_area :description %><br>
<%#= f.label :city_name %>
<%#= f.text_field :city_name %>
<%= f.label :title, "Event Date" %>
<%= f.date_field :datetime %><br>
<%= f.collection_select :city_id, City.all, :id, :name %><br>
<%#= f.label :genre_name %>
<%#= f.text_field :genre_name %>
<%= f.collection_select :genre_id, Genre.all, :id, :name %><br>
<%= f.label :host_id %>
<%= f.collection_select :host_id, User.all, :id, :name %>
<% @instruments.each do |instrument| %>
<%= check_box_tag("instruments[instrument]") %>
<%= label_tag(instrument, instrument.name) %>
<% end %>
<%= f.submit %>
<% end %>